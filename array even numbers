// find even number from elemets in array
#include<stdio.h>
int main(){
    int arr[5],i;
    for(i=0;i<6;i++){
        printf("Enter %d number=",i+1);
        scanf("%d",&arr[i]);
        }
        printf("even numbers are:");
           for(i=0;i<6;i++){
               if(arr[i]%2==0){
            printf("%d\n",arr[i]);
        }}
        return(0);
}
