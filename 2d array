 //2-d array
 #include<stdio.h>
 int main(){
     int arr[3][3],i,j;
     printf("Enter elements for array :");
     for(i=0;i<3;i++){//let i be row counter
     for(j=0;j<3;j++){//j be column
         scanf("%d",&arr[i][j]);
     }
 }
 for(i=0;i<3;i++){
     for(j=0;j<3;j++){
         printf("%d ",arr[i][j]);
     }printf("\n");
 }
     return(0);
 }

  //2-d array additio  
 #include<stdio.h>
 int main(){
     int arr[3][3],jrr[3][3],srr[3][3],i,j;
     printf("Enter elements for 1st array :");
     for(i=0;i<3;i++){//let i be row counter
     for(j=0;j<3;j++){//j be column
         scanf("%d",&arr[i][j]);
     }
 }
 for(i=0;i<3;i++){
     for(j=0;j<3;j++){
         printf("%d ",arr[i][j]);
     }printf("\n");
 }
 printf("Enter elements for 2nd array :");
     for(i=0;i<3;i++){//let i be row counter
     for(j=0;j<3;j++){//j be column
         scanf("%d",&jrr[i][j]);
     }
 }
 for(i=0;i<3;i++){
     for(j=0;j<3;j++){
         printf("%d ",jrr[i][j]);
     }printf("\n");
 }
 printf("sum array will be will be:\n");
 for(i=0;i<3;i++){
     for(j=0;j<3;j++){
         srr[i][j]=arr[i][j]+jrr[i][j];
         printf("%d ",srr[i][j]);
     }printf("\n");
 }
     return(0);
 }
//MULTIPLICATION OF TWO ARRAYs
#include<stdio.h>
int main(){
    int arr[3][3],i,j,k,jrr[3][3],mul[3][3];
    printf("Enter elemnts for 1st array:\n");
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            scanf("%d",&arr[i][j]);
        }
    }
     for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            printf("%d ",arr[i][j]);
        }printf("\n");
    }
     printf("Enter elemnts for 2nd array:\n");
     for(int j=0;j<3;j++){
        for(int k=0;k<3;k++){
            scanf("%d",&jrr[j][k]);
        }
    }
     for(int j=0;j<3;j++){
        for(int k=0;k<3;k++){
            printf("%d ",jrr[j][k]);
        }printf("\n");
    }
    printf("Multiple array of 2 arrays will be:\n");
  for(int i=0;i<3;i++){   //multiple ka valaue laaga rahe
           for(int k=0;k<3;k++){
             mul[i][k]=0;
            for(int j=0;j<3;j++){
            mul[i][k]=mul[i][k]+arr[i][j]*jrr[j][k];
                }
        }
    }
    for(int i=0;i<3;i++){
        for(int k=0;k<3;k++){
            printf("%d ",mul[i][k]);
        }printf("\n");
    }
}
//SUM OF DIAGONAL ELEMNTS WILL BE
#include<stdio.h>
int main(){
    int arr[3][3],i,j,sum;
    printf("Enetr 9 elemnts for array:\n");
    for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            scanf("%d",&arr[i][j]);
        }
    }
     for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            printf("%d ",arr[i][j]);
        }printf("\n");
           }
        printf("Sum of diagonals of elemnt:");
        for(i=0;i<3;i++){
            sum=0;
        for(j=0;j<3;j++){
            sum=sum+arr[j][j];
        }}
            printf("%d",sum);
    }
//Tranpose 
#include<stdio.h>
int main(){
    
     int arr[2][2],i,j;
     printf("Enter elements for array :");
     for(i=0;i<2;i++){//let i be row counter
     for(j=0;j<2;j++){//j be column
         scanf("%d",&arr[i][j]);
     }
 }
 for(i=0;i<2;i++){
     for(j=0;j<2;j++){
         printf("%d ",arr[i][j]);
     }printf("\n");
 }printf("\n");
 printf("Tranpose array:\n");
 for(j=0;j<2;j++){
     for(i=0;i<2;i++){
         printf("%d ",arr[i][j]);
     }printf("\n");
 }
}
