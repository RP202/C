//SUM OF ALL LEMENTS 
#include<stdio.h>
int main(){
    int arr[20],n,i,sum;
    sum=0;
    printf("Enter Limit for array:");
    scanf("%d",&n);
    printf("enter lements of array:");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    printf("Elments of array are:\n");
      for(i=0;i<n;i++){
        printf("%d ",arr[i]);
    }printf("\n");
      for(i=0;i<n;i++){
       sum=sum+arr[i];
    }
    printf("%d is sum of all elemnts of array",sum);
}
//sumof 2 ARRAYS
#include<stdio.h>
int main(){
    int arr[20],brr[20],n,i,m,sum;
    sum=0;
    printf("Enter Limit for both arrays :");
    scanf("%d %d",&n,&m);
    printf("enter lements of array 1:");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    printf("Elments of array are 1:\n");
      for(i=0;i<n;i++){
        printf("%d ",arr[i]);
    }printf("\n");
    printf("enter lements of array 2:");
    for(i=0;i<m;i++){
        scanf("%d",&brr[i]);
    }
    printf("Elments of array are 2:\n");
      for(i=0;i<n;i++){
        printf("%d ",brr[i]);
    }printf("\n");
    for(i=0;i<5;i++){
       sum=sum+arr[i]+brr[i];
    } printf("%d is sum of all elemnts",sum);
   
}
//largest elemnts
#include<stdio.h>
int main(){
    int arr[20],n,i,m;
    printf("Enter Limit for array:");
    scanf("%d",&n);
    printf("enter lements of array:");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    printf("Elments of array are:\n");
      for(i=0;i<n;i++){
        printf("%d ",arr[i]);
    }printf("\n");
    m=arr[0];
     for(i=0;i<n;i++){
    if(m<arr[i]){
        m=arr[i];
    }
    }printf("%d is largest elemnt",m);
      
}
